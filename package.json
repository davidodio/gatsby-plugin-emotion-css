{
	"name": "@davidodio/gatsby-plugin-emotion-css",
	"description": "Gatsby plugin to provide support for the @emotion/babel-preset-css-prop",
	"version": "1.0.0-beta.1",
	"main": "dist/index.js",
	"author": "David ODonnell <code@davidod.io>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/davidodio/gatsby-plugin-emotion-css.git"
	},
	"bugs": {
		"url": "https://github.com/davidodio/gatsby-plugin-emotion-css/issues"
	},
	"homepage": "https://github.com/davidodio/gatsby-plugin-emotion-css",
	"keywords": [
		"gatsby",
